#lang racket      ;; define a linguagem default
; ------------------------------------------------
(display "m é um char? ") (char? "m")
(display "14 é um char? ") (char? 14)
(display "#\b é um char? ") (char? #\b)
(display " é um char? ") (char? #\m)
